@model CreaturesViewModel
@{
    ViewData["Title"] = "Add";
}

<h1>Add</h1>

<form asp-controller="Pokedex" asp-action="Add" method="post">
    @Html.LabelFor(m => m.Name)
    @Html.TextBoxFor(m => m.Name)
    @Html.LabelFor(m => m.DexNum)
    @Html.TextBoxFor(m => m.DexNum)
    <input type="submit" value="Submit"/>
</form>
